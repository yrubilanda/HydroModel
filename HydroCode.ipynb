   "source": [
    "# Convert max_elevation to integer\n",
    "max_elevation = int(float(max_elevation))\n",
    "\n",
    "# Iterate over each input raster file, each reservoir DEM\n",
    "for ClippedReservoir in ClippedReservoirs:\n",
    "    # Iterate over 1-meter increments up to the maximum elevation\n",
    "    for elevation_value in range(289, int(max_elevation) + 1):\n",
    "        output_raster_file = f\"{ClippedReservoir[:-4]}_elev_{elevation_value}.tif\"  \n",
    "        # Apply raster calculator operation Con('DEM.tif' <= \"wanted elevation\", 1, 0)\n",
    "        #if DEM is less than or equal to wanted elev assign value of 1, otherwise assign value of 0\n",
    "        arcpy.gp.RasterCalculator_sa(f'Con(\"{ClippedReservoir}\" <= {elevation_value}, 1, 0)', output_raster_file)\n",
    "\n",
    "print(\"Raster calculator operations completed.\")"
   ]
