    "# List to store maximum elevations\n",
    "max_elevations = []\n",
    "\n",
    "# Iterate over each input raster file\n",
    "for ClippedReservoir in ClippedReservoirs:\n",
    "    # Determine maximum elevation value\n",
    "    max_elevation = arcpy.GetRasterProperties_management(ClippedReservoir, \"MAXIMUM\").getOutput(0)\n",
    "\n",
    "    # Append maximum elevation to the list\n",
