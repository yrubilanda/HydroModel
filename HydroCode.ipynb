      "Clipping complete.\n"
     ]
    }
   ],
   "source": [
    "#input DEM raster\n",
    "input_dem = \"TikalDEM.tif\"\n",
    "\n",
    "#input shapefile with multiple features\n",
    "input_shapefile = \"TikalReservoirsWB.shp\"\n",
    "\n",
    "#output folder\n",
    "output_directory = r\"C:\\Users\\yesen\\OneDrive\\Documents\\Academic\\Projects\\HydroModels\\Tikal\"\n",
    "\n",
    "# Iterate through each feature in the shapefile\n",
    "#search cursor \"arcpy.da.SearchCursor\" goes through the input shapefile \"tikalreservoirs\" and retrieves its shape and ID\n",
    "with arcpy.da.SearchCursor(input_shapefile, [\"SHAPE@\", \"FID\"]) as cursor:\n",
    "    \n",
    "    #Loop that goes through each row that was shown in search cursor, pulling from \"SHAPE@\" and FID\n",
    "    for row in cursor:\n",
    "        # Get the feature geometry from the row which is pulled from row[0] = geometry\n",
    "        geometry = row[0]\n",
    "        \n",
    "        #attribute value corresponds to FID\n",
    "        value = row[1]\n",
    "        \n",
    "        #output extent filename: reservoir{value}.tif ex reservoir1.tif, reservoir2.tif\n",
    "        output_clipped_dem = os.path.join(output_directory, f\"reservoir{value}.tif\")\n",
    "        \n",
    "        #extractbymask will extract raster that corresponds to the geometry from above\n",
    "        arcpy.sa.ExtractByMask(input_dem, geometry).save(output_clipped_dem)\n",
    "\n",
    "print(\"Clipping complete.\")"
   ]
  },
