    "#clip DEM to reservoir shapefile EXTENT NOT SHAPE\n",
    "#input DEM raster\n",
    "TikalDEM = \"TikalDEM.tif\"\n",
    "\n",
    "#input shapefile extent\n",
    "TikalReservoirs = \"TikalReservoirsWB.shp\"\n",
    "\n",
    "#output raster\n",
    "TikalReservoirDEM = \"clipped_ReservoirDEM.tif\"\n",
    "\n",
    "arcpy.management.Clip(TikalDEM, \"#\", TikalReservoirDEM, TikalReservoirs, \"#\" \"ClippingGeometry\")\n",
    "\n",
    "print(\"DEM clipped successfully.\")"
   ]
